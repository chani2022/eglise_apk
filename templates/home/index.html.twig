						{# {% set date = calendrier.updatedAt|date('d M Y') %} #}
						{% set titre = calendrier.titre %}
						{% set dayEvent = calendrier.eventAt|date('d') %}
						{% set monthEvent = calendrier.eventAt|date('F') %}
						{% set hoursEvent = calendrier.eventAt|date('H:i') %}
						<div class="item">
							<div class="event-bx">
								<div class="action-box">
									<img src="{{ asset (image) }}" alt="photo">
								</div>
								<div class="info-bx d-flex">
									<div>
										<div class="event-time">
											<div class="event-date">{{ dayEvent }}</div>
											<div class="event-month">{{ monthEvent }}</div>
										</div>
									</div>

									<div class="event-info">
