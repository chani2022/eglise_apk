											<a href="#" class="comments-bx comments post-populaire" id="{{ populaire_recent.getId() }}" data-toggle="modal" data-target="#comments" id=" {{ populaire_recent.id }} " data-date=" {{ populaire_recent.updatedAt|date(" m-d-y") }} " data-titre=" {{ populaire_recent.titre }} " data-commentaire=" {{ populaire_recent.commentaire }} " data-image="article/{{ populaire_recent.image }} " data-auteur=" {{ populaire_recent.user.nom ? populaire_recent.user.nom ~' '~ populaire_recent.user.prenom: populaire_recent.user.username }} ">
												<i class="fa fa-comments-o"></i>
												{{ populaire_recent.comments|length }}
												Commentaire
											</a>
										</li>
									</ul>
									<div class="widget widget_gallery gallery-grid-4">
										<h6 class="widget-title">{% trans %}Galery{% endtrans %}
										</h6>
										<ul class="magnific-image galerie-post">
											{% for gal in populaire_recent.galerie %}
												<li>
													<a href="{{ asset('galerie/'~ gal.nomImage) }}" class="magnific-anchor"><img src={{ asset('galerie/'~ gal.nomImage) }} alt=""></a>
												</li>
											{% endfor %}
										</ul>
									</div>

